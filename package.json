{
  "name": "intellitech-solutions",
  "version": "1.0.0",
  "description": "IntelliTech Solutions - Full Stack Application",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "echo 'Backend built successfully'",
    "start": "npm run start:backend",
    "start:backend": "cd backend && npm start",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "vercel-build": "cd frontend && npm run build"
  },
  "devDependencies": {
    "concurrently": "^7.6.0"
  },
  "keywords": ["nextjs", "express", "mongodb", "dashboard", "crm"],
  "author": "Nkosiyazi Gwili",
  "license": "MIT"
}